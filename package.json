{
	"name": "@liphu/liphu-ui",
	"version": "0.1.0",
	"author": "Jordhan Saez <jordhan.sm@gmail.com> (http://github.com/hwigaro)",
	"description": "A Vue.js UI-Kit made with â™¥ by Liphu",
	"keywords": [
		"vue",
		"vue2",
		"vuejs",
		"vue.js",
		"library",
		"ui",
		"ui-kit",
		"kit",
		"style",
		"component",
		"components"
	],
	"license": "MIT",
	"main": "./dist/liphu-ui.common.js",
	"files": ["dist/*", "src/*", "./*.json", "./*.js"],
	"homepage": "https://github.com/LiphuCL/liphu-ui",
	"bugs": {
		"url": "https://github.com/LiphuCL/liphu-ui/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/LiphuCL/liphu-ui.git"
	},
	"scripts": {
		"build": "cross-env NODE_ENV=production webpack --config ./build/webpack.config.js --env.modules=common,umd,theme,components,utils,site",
		"build:common": "cross-env NODE_ENV=production webpack --config ./build/webpack.config.js --env.modules=common",
		"build:umd": "cross-env NODE_ENV=production webpack --config ./build/webpack.config.js --env.modules=umd",
		"build:theme": "cross-env NODE_ENV=production webpack --config ./build/webpack.config.js --env.modules=theme",
		"build:components": "cross-env NODE_ENV=production webpack --config ./build/webpack.config.js --env.modules=components",
		"build:utils": "cross-env NODE_ENV=production webpack --config ./build/webpack.config.js --env.modules=utils",
		"build:site": "cross-env NODE_ENV=production webpack --config ./build/webpack.config.js --env.modules=site",
		"dev": "cross-env NODE_ENV=development webpack-dev-server --config ./build/webpack.config.js --env.modules=site",
		"lint:code": "prettier-eslint --write './{site/src,src,test,build}**/*.{js,jsx,vue,json},./site/*.{js,jsx,vue,json}'",
		"lint:style": "prettier-stylelint --write -q './{site/src,src}/**/*.{css,scss,vue}'",
		"lint": "yarn run lint:code && yarn run lint:style",
		"test": "cross-env NODE_ENV=test BABEL_ENV=test nyc --cache=false mocha-webpack --webpack-config ./build/webpack.config.test.js --require './test/setup.js' './test/specs/*.spec.js'",
		"test:watch": "cross-env NODE_ENV=test BABEL_ENV=test mocha-webpack -w --webpack-config ./build/webpack.config.test.js --require './test/setup.js' './test/specs/*.spec.js'",
		"stylelint-check": "stylelint-config-prettier-check",
		"eslint-check": "eslint --print-config .eslint.js | eslint-config-prettier-check",
		"version": "auto-changelog -p && git add CHANGELOG.md",
		"heroku-postbuild": "yarn run build:site"
	},
	"nyc": {
		"include": ["src/**/*.{js,vue}"],
		"exclude": ["test/**/*.spec.js", "build", "dist", "site/dist"],
		"reporter": ["lcov", "text", "html"],
		"extension": [".js", ".vue"],
		"instrument": true,
		"sourceMap": true
	},
	"husky": {
		"hooks": {
			"pre-commit": "yarn run lint && yarn run test && yarn run version"
		}
	},
	"auto-changelog": {
		"output": "CHANGELOG.md",
		"template": "changelog.tpl.hbs",
		"unreleased": true,
		"commitLimit": false,
		"replaceText": {}
	},
	"dependencies": {
		"connect-history-api-fallback": "^1.6.0",
		"consola": "^2.4.1",
		"express": "^4.16.4",
		"serve-static": "^1.13.2",
		"stylelint-scss": "^3.5.3",
		"vue": "^2.6.6",
		"vue-i18n": "^8.8.1",
		"vue-router": "^3.0.2",
		"vuex": "^3.1.0"
	},
	"devDependencies": {
		"@babel/core": "^7.2.2",
		"@babel/plugin-syntax-dynamic-import": "^7.2.0",
		"@babel/plugin-syntax-jsx": "^7.0.0",
		"@babel/plugin-transform-runtime": "^7.2.0",
		"@babel/plugin-transform-spread": "^7.2.2",
		"@babel/preset-env": "^7.3.1",
		"@babel/register": "^7.0.0",
		"@vue/test-utils": "^1.0.0-beta.29",
		"babel-eslint": "^10.0.1",
		"babel-helper-vue-jsx-merge-props": "^2.0.3",
		"babel-loader": "^8.0.5",
		"babel-plugin-istanbul": "^5.1.1",
		"babel-plugin-module-resolver": "^3.2.0",
		"babel-plugin-syntax-jsx": "^6.18.0",
		"babel-plugin-transform-vue-jsx": "^3.7.0",
		"babel-preset-minify": "^0.5.0",
		"clean-webpack-plugin": "^1.0.1",
		"copy-webpack-plugin": "^4.6.0",
		"cross-env": "^5.2.0",
		"css-loader": "^2.1.0",
		"cssnano": "^4.1.8",
		"eslint": "^5.13.0",
		"eslint-config-prettier": "^4.0.0",
		"eslint-plugin-mocha": "^5.3.0",
		"eslint-plugin-prettier": "^3.0.1",
		"eslint-plugin-vue": "^5.1.0",
		"expect": "^24.1.0",
		"extract-text-webpack-plugin": "^4.0.0-beta.0",
		"file-loader": "^3.0.1",
		"html-webpack-plugin": "^3.2.0",
		"husky": "^1.3.1",
		"istanbul-instrumenter-loader": "^3.0.1",
		"jsdom": "^13.2.0",
		"jsdom-global": "^3.0.2",
		"mini-css-extract-plugin": "^0.5.0",
		"mocha": "^5.2.0",
		"mocha-webpack": "^2.0.0-beta.0",
		"node-sass": "^4.11.0",
		"nyc": "^13.3.0",
		"parallel-webpack": "^2.3.0",
		"postcss-import": "^12.0.1",
		"postcss-loader": "^3.0.0",
		"postcss-preset-env": "^6.5.0",
		"prettier": "^1.13.7",
		"prettier-eslint": "^8.8.2",
		"prettier-eslint-cli": "^4.7.1",
		"prettier-stylelint": "^0.4.2",
		"resolve-url-loader": "^3.0.1",
		"sass-loader": "^7.1.0",
		"style-loader": "^0.23.1",
		"stylelint": "^9.10.1",
		"stylelint-config-prettier": "^4.0.0",
		"stylelint-prettier": "^1.0.6",
		"terser-webpack-plugin": "^1.2.2",
		"url-loader": "^1.1.2",
		"vue-loader": "^15.6.4",
		"vue-style-loader": "^4.1.2",
		"vue-template-compiler": "^2.6.6",
		"webpack": "^4.29.3",
		"webpack-cli": "^3.2.3",
		"webpack-dev-server": "^3.1.14",
		"webpack-node-externals": "^1.7.2",
		"webpackbar": "^3.1.5"
	}
}
